<style is:global lang="scss">
	html {
		width: 100vw;
	}
	* {
		margin: 0;
		padding: 0;
		box-sizing: border-box;
		font-family: "Poppins", sans-serif;
		scroll-behavior: smooth;
		cursor: none;
	}

	body {
		height: 10000px;
		overflow-x: hidden;
		width: 100vw;
		background-size: 100% 100%;
		background-position:
			0px 0px,
			0px 0px,
			0px 0px;
		// background: url("https://cdn.discordapp.com/attachments/973568724722389052/1187782896996601916/MineBest_website_Mesa_de_trabajo_1.png?ex=65982406&is=6585af06&hm=9abdec925815c5075e3cb79a499bfc28612b5abc3cce1385cb65e428b8cd98c1&");
		display: flex;
		flex-direction: column;
		cursor: none;

		&::-webkit-scrollbar {
			width: 7px;
			color: #161616;
		}

		&::-webkit-scrollbar-track {
			background-color: #161616;
		}

		&::-webkit-scrollbar-thumb {
			border-radius: 30px;
			transition: all 0.5s;
			background: #ff7e2e;
		}
	}

	.btc {
		min-height: 100vh;
		display: flex;
		justify-content: center;
		align-items: center;
		background-color: #141414;
		position: relative;
		z-index: 30;
	}

	.no-btc {
		display: none;
	}

	.email-popup {
		width: 100vw;
		height: 100vh;
		/* From https://css.glass */
		background: rgba(9, 8, 12, 0.53);
		border-radius: 16px;
		box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);
		backdrop-filter: blur(16.1px);
		-webkit-backdrop-filter: blur(16.1px);
		border: 1px solid rgba(9, 8, 12, 0.3);
		position: absolute;
		z-index: 9;
		display: none;
		justify-content: center;
		align-items: center;
	}

	.popup {
		display: flex;
		flex-direction: column;
		align-items: center;
		font-family: "Poppins";
		gap: 20px;
		/* From https://css.glass */
		background: rgba(13, 14, 35, 0.61);
		border-radius: 16px;
		box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);
		backdrop-filter: blur(14.7px);
		-webkit-backdrop-filter: blur(14.7px);
		border: 1px solid rgba(13, 14, 35, 0.3);
		padding-left: 100px;
		padding-right: 100px;
		padding-top: 60px;
		padding-bottom: 60px;

		&__container {
			text-align: center;
		}

		&__close-btn {
			color: #fff;
			position: absolute;
			top: 20px;
			right: 40px;
		}

		&__img {
			position: relative;
			width: 6vw;
		}

		&__title {
			color: white;
			font-weight: 700;
		}

		&__subtitle {
			font-size: 16px;
			font-weight: 400;
			color: #fff;
		}

		&__txt {
			font-weight: 600;
			color: #ff843f;
		}

		&__wtsp-btn {
			padding: 10px;
			width: 100%;
			border-radius: 50px;
			border: none;
			outline: none;
			background-color: #d9d9d9;
			color: #1cb05f;
			font-weight: 600;
			display: flex;
			align-items: center;
			justify-content: center;

			&__icon {
				width: 3vw;
			}

			&__link {
				text-decoration: underline;
			}
		}
	}

	a {
		text-decoration: none;
	}

	::selection {
		background-color: #ff7e2e;
		color: #fff;
	}

	hr {
		margin: 10vh;
		align-self: center;
		width: 70vw;
		border: 1px solid #564965fa;
	}

	@keyframes go-m-t {
		0% {
			top: 302px;
		}
		100% {
			top: 0px;
		}
	}

	@keyframes go-m-b {
		0% {
			top: 0px;
		}
		100% {
			margin-bottom: 300px;
			top: 402px;
		}
	}

	@media only screen and (max-width: 480px) {
		// hr{
		// 	margin: 30px;
		// 	align-self: center;
		// 	width: 70vw;
		// 	border: 1px solid #564965fa;
		// }
	}

	.article__title {
		font-size: clamp(35px, 3.5vw + 1vh, 80px);
	}

	.cursor-dot {
		width: 15px;
		height: 15px;
		background-color: #ffe602;
	}

	.cursor-outline {
		width: 60px;
		height: 60px;
		border: 2px solid #ffe602;
	}

	.cursor-dot,
	.cursor-outline {
		position: fixed;
		top: 0px;
		left: 0px;
		transform: translate(-50%, -50%);
		border-radius: 50%;
		z-index: 10;
		pointer-events: none;
	}

	#particles-js {
		position: fixed;
		z-index: 9;
		height: 100%;
		width: 100%;
		z-index: 1;
	}

	.secret {
		font-size: 1px;
	}

	@media only screen and (max-width: 900px) {
		.cursor-dot,
		.cursor-outline {
			display: none;
		}
	}

	@media only screen and (min-width: 1600px) {
		.popup {
			&__wtsp-btn {
				padding: 4px;

				&__icon{
					width: 2.5vw;
				}
			}
		}
	}
</style>
<!--Cursor -->
<script is:inline>
	const cursorDot = document.querySelector(".cursor-dot");
	const cursorOutline = document.querySelector(".cursor-outline");

	window.addEventListener("mousemove", (evt) => {
		const posX = evt.clientX;
		const posY = evt.clientY;

		cursorDot.style.left = `${posX}px`;
		cursorDot.style.top = `${posY}px`;

		cursorOutline.animate(
			{
				left: `${posX}px`,
				top: `${posY}px`,
			},
			{ duration: 500, fill: "forwards" },
		);
	});
</script>

